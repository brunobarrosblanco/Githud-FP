Module OpAsicnacion
    Sub Main()
        'Operadores asignación: =, +=, -=, *=, /=

        Dim x As Integer = 1
        'Sumar 1 a x --> x = x + 1
        x -= 1
        Console.WriteLine("x = " & x)
        'Resto 1 a x --> x = x - 1
        x -= 1
        Console.WriteLine()

    End Sub

End Module
